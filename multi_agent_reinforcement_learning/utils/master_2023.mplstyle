# master_1823.mplstyle

### FIGURE
figure.facecolor: white
figure.dpi: 100
figure.titlesize: 30
figure.figsize: 14, 10

### LINES
lines.linewidth: 2.5

### LEGEND
legend.fontsize: 24

### TICKS
xtick.direction: out
ytick.direction: out

### FONTS
font.family: sans-serif
font.size: 28

### AXES
axes.grid: False
axes.facecolor: white
axes.linewidth: 1.0
axes.titlesize: 30
axes.titlepad: 18
axes.titleweight: bold
axes.labelsize: 28
axes.spines.top: True
axes.spines.right: True
axes.prop_cycle: cycler('color', ["#8C1C13", "#2F4550", "#A3BBAD", "#C49991", "#F7A072"])
