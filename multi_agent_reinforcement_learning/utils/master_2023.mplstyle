# master_2023.mplstyle

### FIGURE
figure.facecolor: white
figure.dpi: 100
figure.titlesize: 14

### LINES
lines.linewidth: 1.5

### LEGEND
legend.fontsize: 11

### TICKS
xtick.direction: out
ytick.direction: out

### FONTS
font.family: sans-serif
font.size: 11

### AXES
axes.grid: False
axes.facecolor: white
axes.linewidth: 1.0
axes.titlesize: 20
axes.titlepad: 20
axes.titleweight: bold
axes.labelsize: 11
axes.spines.top: True
axes.spines.right: True
axes.prop_cycle: cycler('color', ["#8C1C13", "#2F4550", "#A3BBAD", "#C49991", "#F7A072"])
